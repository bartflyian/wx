page{
    height: 100%;
    }
.page{
    background: #101c21;
    height: 100%;
    padding-top: 25px;
}  
.page .section_nav{
    color: #fff;
    margin: 0px 50px;
    border: 1px solid #fff;
    border-radius: 50px; 
}
.page .section_nav view{ 
    width: 33.3%; 
    text-align: center;
    display: inline-block;
    line-height: 30px;
}
.page .section_nav .on{ 
    color: rgb(248, 175, 16); 
}

.tab-swiper{
    color: #fff;
    height: 100%; 
}
.tab-swiper .section_positions{
    margin: 20px 30px 0 30px;
} 
.tab-swiper .position_list image{
    width: 30px;
    height: 30px;  
    vertical-align: middle;
}
.position_list{
    width: 100%;
    height: 45px;
    border-bottom: 1px solid  #696d6f; 
    margin-top: 10px;
}
.tab-swiper .position_list text{
    width: 20%;
    height: 30px;
    text-align: center; 
    display: inline-block;
} 
.tab-swiper .position_list .rank{
    width: 15%;
}
.tab-swiper .position_list .teamName{
    width: 35%;
}
